<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pcbuildhub.oosd.pcbuildhub">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginCheck">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistrationCheck" />
        <activity android:name=".ProvidePreference" />
        <activity android:name=".System_recommendation" />
        <activity android:name=".AddConfiguration" />
        <activity android:name=".UpdateConfiguration" />
        <activity android:name=".DeleteConfiguration" />
        <activity android:name=".Modify_recommendations" />
        <activity android:name=".UpdateSingleConfiguration"></activity>
    </application>

</manifest>